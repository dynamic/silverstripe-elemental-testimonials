{
    "name": "dynamic/silverstripe-elemental-testimonials",
    "description": "Testimonials element for Silverstripe Elemental",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "testimonials"
    ],
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/dynamic"
        }
    ],
    "require": {
        "dnadesign/silverstripe-elemental": "^5.0",
        "silverstripe/framework": "^5",
        "symbiote/silverstripe-gridfieldextensions": "^4.0"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\Elements\\": "src/",
            "Dynamic\\Elements\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/"
    }
}
